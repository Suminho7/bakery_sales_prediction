    "import pandas as pd\n",
    "import numpy as np\n",
    "from sklearn.preprocessing import StandardScaler\n",
    "from sklearn.model_selection import train_test_split\n",
    "import tensorflow as tf\n",
    "from tensorflow.keras.models import Sequential\n",
    "from tensorflow.keras.layers import Dense\n",
    "\n",
    "\n",
    "\n",
    "# Dummy-Daten für das Beispiel\n",
    "np.random.seed(0)\n",
    "merged_data = pd.DataFrame({\n",
    "    'Umsatz': np.random.rand(100) * 1000,\n",
    "    'Warengruppe': np.random.choice(['A', 'B', 'C'], 100),\n",
    "    'Wochentag_Numerisch': np.random.choice(range(7), 100),\n",
    "    'Temperatur': np.random.rand(100) * 30,\n",
    "    'Wochenende': np.random.choice([0, 1], 100),\n",
    "    'KielerWoche': np.random.choice([0, 1], 100),\n",
    "    'UmsatzVortag': np.random.rand(100) * 1000\n",
    "})\n",
    "\n",
    "# One-Hot-Encoding für kategoriale Variablen\n",
    "merged_data = pd.get_dummies(merged_data, columns=['Warengruppe', 'Wochentag_Numerisch'], drop_first=True)\n",
    "\n",
    "# Erstellen der Interaktionsvariablen\n",
    "for warengruppe in ['A', 'B', 'C']:\n",
    "    if warengruppe != 'A':  # 'A' ist die Basisgruppe\n",
    "        merged_data[f'Warengruppe_{warengruppe}'] = merged_data[f'Warengruppe_{warengruppe}']\n",
    "        merged_data[f'Warengruppe_{warengruppe}_KielerWoche'] = merged_data[f'Warengruppe_{warengruppe}'] * merged_data['KielerWoche']\n",
    "        merged_data[f'Warengruppe_{warengruppe}_Wochenende'] = merged_data[f'Warengruppe_{warengruppe}'] * merged_data['Wochenende']\n",
    "        merged_data[f'Warengruppe_{warengruppe}_UmsatzVortag'] = merged_data[f'Warengruppe_{warengruppe}'] * merged_data['UmsatzVortag']\n",
    "        for wochentag in range(1, 7):  # 0 ist die Basisgruppe\n",
    "            merged_data[f'Warengruppe_{warengruppe}_Wochentag_{wochentag}'] = merged_data[f'Warengruppe_{warengruppe}'] * merged_data[f'Wochentag_Numerisch_{wochentag}']\n",
    "\n",
    "# Definieren der Ziel- und Eingangsvariablen\n",
    "y = merged_data['Umsatz'].values\n",
    "X = merged_data.drop(columns=['Umsatz']).values\n",
    "\n",
    "# Normalisierung der Eingangsvariablen\n",
    "scaler = StandardScaler()\n",
    "X_scaled = scaler.fit_transform(X)\n",
    "\n",
    "# Aufteilen der Daten in Trainings- und Testsets\n",
    "X_train, X_test, y_train, y_test = train_test_split(X_scaled, y, test_size=0.2, random_state=0)\n",
    "\n",
    "# Erstellen des neuronalen Netzes\n",
    "model = Sequential()\n",
    "model.add(Dense(64, input_dim=X_train.shape[1], activation='relu'))\n",
    "model.add(Dense(32, activation='relu'))\n",
    "model.add(Dense(1))  # Für eine Regression mit einem Zielwert\n",
    "\n",
    "# Kompilieren des Modells\n",
    "model.compile(optimizer='adam', loss='mean_squared_error')\n",
    "\n",
    "# Training des Modells\n",
    "model.fit(X_train, y_train, epochs=100, batch_size=10, validation_data=(X_test, y_test))\n",
    "\n",
    "# Evaluieren des Modells\n",
    "loss = model.evaluate(X_test, y_test)\n",
    "print(f'Test Loss: {loss}')\n",
    "\n",
    "# Vorhersagen mit dem trainierten Modell\n",
    "predictions = model.predict(X_test)\n",
    "\n",
    "model.summary()\n"
   ]
  }
